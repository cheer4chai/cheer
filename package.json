{
  "name": "cheer",
  "version": "0.2.2",
  "description": "A tool library with webpack and es6",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
