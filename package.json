    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "yargs": "7.1.0"
  },
  "repository": {
